let msg = "secert information";
class User{
    constructor(name,age){
        this.name = name;
        this.age = age;
    }
    viewData(){
        console.log("DATA IS:",msg);
    }
}
class Admin extends User{
    constructor(name,age){
        super(name,age);
    }
    editdata(){
        msg = "new information";
    }
}
let student1 = new User("ashu","20");
let student2 = new User("dhoni","40");

let admin = new Admin("basu","33");
